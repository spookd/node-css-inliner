  },
  "dependencies": {
    "async": "~0.8.0",
    "clean-css": "~2.1.8",
    "coffee-script": "~1.7.1",
    "commander": "^2.2.0",
    "css": "~1.6.0",
    "domino": "^1.0.17",
    "generic-pool": "~2.0.4",
    "lodash": "^2.4.1",
    "node-phantom-simple": "~1.2.0",
    "phantomjs": "~1.9.7-5",
    "request": "~2.34.0"
